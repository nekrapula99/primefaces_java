	private void colaEstandar() {
		try {
			mregcg00.setUseegc(codusu);
			mregcg00.setPrgegc("Compañias " + getClass().getName());
			mregcg00.setFeaegc(new Date());
			mregcg00.setMaqegc(ip);

		} catch (Exception e) {
