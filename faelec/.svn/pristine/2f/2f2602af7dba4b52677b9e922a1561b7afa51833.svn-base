		    Integer mayor = Collections.max(listMtiptx00Temp);
		    sufijo = mayor+1;
		    
		    Date date = new Date();
		    
		 // el que parsea
		    SimpleDateFormat parseador = new SimpleDateFormat("dd-MM-yy");
		    // el que formatea
		    SimpleDateFormat formateador = new SimpleDateFormat("yyMMdd");
		    fechaActual = formateador.format(date);
	
	} catch (Exception e) {
