							</p:dataTable>
						</h:form>
					
					<h:form id="formLogo">		
						<p:remoteCommand name="rcLogo" actionListener="#{seMgente00.uploadFirmaDigitalFile}"  update="formDetalle:tabView:pnlDetalleLogo" />				
					</h:form>	
					
					
					
					
					
