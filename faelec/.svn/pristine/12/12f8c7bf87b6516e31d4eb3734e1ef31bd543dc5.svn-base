											filterBy="#{mgente00.nrnite}" filterMatchMode="contains"  width="150">
									<h:outputText value="#{mgente00.nrnite}"/>
								</p:column>
								
								 <!-- Departamento -->
								<p:column headerText="#{seMgente00.idiomasHm.get('iddpte')}" sortBy="#{mgente00.iddpte}"
											filterBy="#{mgente00.mgenus002.dcttus}" filterMatchMode="contains"  width="150">
									<h:outputText value="#{mgente00.mgenus002.dcttus}"/>
								</p:column>
								
								 <!-- Ciudad -->
								<p:column headerText="#{seMgente00.idiomasHm.get('idcite')}" sortBy="#{mgente00.idcite}"
											filterBy="#{mgente00.mgenus003.dcttus}" filterMatchMode="contains"  width="150">
									<h:outputText value="#{mgente00.mgenus003.dcttus}"/>
								</p:column>
								
