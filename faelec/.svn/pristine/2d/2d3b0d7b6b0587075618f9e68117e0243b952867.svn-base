	public void accionNuevo() {
		try {
			setAccion(Constantes.ACCION_NUEVO);
			mfunfu00 = new Mfunfu00();
			mfunfu00.setRegufu(true);
			reiniciarListasMaestro();
		} catch (Exception e) {
