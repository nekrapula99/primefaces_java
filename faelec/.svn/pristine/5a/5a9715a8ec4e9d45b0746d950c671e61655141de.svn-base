<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:p="http://primefaces.org/ui"
                template="/WEB-INF/template.xhtml"
                xmlns:pe="http://primefaces.org/ui/extensions">

    <ui:define name="content">
    
        <div class="ui-g">
        
            <div class="ui-g-12">
                  
                <div class="card">
                	<h1>#{seTqigqg00.titulo}</h1>
					
									<!-- //////////////////////////////////////////////////////////////////////////////// -->
									<!-- //////////////////////////////CABECERA////////////////////////////////////////// -->
									<h:form id="formCabecera">
										<p:remoteCommand autoRun="true">
											<f:setPropertyActionListener target="#{seCabecera.objCall}" value="#{seTqigqg00}" />
											<f:setPropertyActionListener target="#{seCabecera.varAdicional}" value="#{tqigqg00}" />
											<f:setPropertyActionListener target="#{seCabecera.pac}" value="pacTqigqg00" />
											<f:setPropertyActionListener target="#{seCabecera.seControl.titulo}" value="#{seTqigqg00.titulo}" />
											<f:event type="preRenderView" listener="#{seTqigqg00.preRenderView}" />
										</p:remoteCommand>
										<ui:include src="/pages/plantillas/cabeceraCotizacion.xhtml" />
									</h:form>
					
									<h:form id="formNuevoUpdate">
										<p:remoteCommand name="rcNuevoUpdate"
											oncomplete="PF('dlgNuevoUpdate').show();"
											actionListener="#{seTqigqg00.accionNuevo}" update=":formDetalle" />
									</h:form>
					
									<!-- //////////////////////////////////////////////////////////////////////// -->
									<!-- //////////////////////////////// DATATABLE ///////////////////////////// -->
									<h:form id="formTable">
										<p:messages id="messages" showDetail="false" autoUpdate="true" closable="true" />
					
										<p:dataTable id="dTable" widgetVar="fTable" var="tqigqg00" reflow="true"
											paginator="true" paginatorPosition="bottom" rows="10"
											rowKey="#{tqigqg00.idtqqg}" scrollable="true"
											emptyMessage="#{mensajes.grid_emptyMessage}"
											value="#{seTqigqg00.listTqigqg00}" selectionMode="single"
											selection="#{seTqigqg00.tqigqg00}"
											filteredValue="#{seTqigqg00.filteredTqigqg00}"
											resizableColumns="true"
											currentPageReportTemplate="#{mensajes.grid_total_registros} {totalRecords}"
											paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink}">
					
											<p:ajax disabled="#{seCabecera.seControl.PERMISOSACCION.get('BTN_ACTUALIZAR')==null}" event="rowSelect" listener="#{seTqigqg00.accionModificar}"
												oncomplete="PF('dlgNuevoUpdate').show();" update=":formDetalle,:formDetalle:tabView:dTableTqdrqr00,:formDetalle:tabView:pnlTqigqg00,:formDetalle:dialogoTqigqg00,:formDetalle:tabView:dTableTqdfqf00" />
					
											<p:column style="width:14px;" exportable="false">
												<f:facet name="header">
													<p:commandLink id="clarFilterLink"	onclick="PF('fTable').clearFilters();"  actionListener="#{seTqigqg00.clearAllFilters}">
													<p:graphicImage value="/resources/images/clean_filters.png"  height="16" title="#{mensajes.boton_limpiar_filtros}" />
													</p:commandLink>
												</f:facet>
												<p:commandLink disabled="#{seCabecera.seControl.PERMISOSACCION.get('BTN_ELIMINAR')==null}" id="deleteLink" update=":formTable" actionListener="#{seTqigqg00.remove}" process="@this">
													<p:graphicImage value="/resources/images/Trash.gif"	style="border:0;" height="16" title="#{mensajes.boton_eliminar}" />
													<p:confirm header="#{mensajes.confirm_titulo}" message="#{mensajes.confirm_mensaje_remove}" icon="ui-icon-alert" />
													<f:attribute name="tqigqg00" value="#{tqigqg00}" />
												</p:commandLink>
											</p:column>
					
											<!-- Numero Cotización -->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('nroqqg')}"
												filterBy="#{tqigqg00.nroqqg}" filterMatchMode="contains"
												width="100" sortBy="#{tqigqg00.nroqqg}">
												<h:outputText value="#{tqigqg00.nroqqg}" />
											</p:column>
					
											<!-- Fecha Creación -->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('fecrqg')}" filterBy="#{tqigqg00.fecrqg}"
												filterMatchMode="contains" sortBy="#{tqigqg00.fecrqg}" width="125">
												<h:outputText value="#{tqigqg00.fecrqg}" >
													<f:convertDateTime pattern="#{seTqigqg00.getSeControlFactura().PARAMETROS.get('For_Fechas')}"/>
												</h:outputText>
											</p:column>
				
					
											<!--Cliente-->
											
											<p:column headerText="#{seTqigqg00.idiomasHm.get('idclqg')}" sortBy="#{tqigqg00.mgente00.dctate}"
														filterBy="#{tqigqg00.mgente00.dctate}}" filterMatchMode="contains"  width="150">
												<h:outputText value="#{tqigqg00.mgente00.dctate}" />
											</p:column>
					
					
											<!-- Prod PPl -->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('idppqg')}" sortBy="#{tqigqg00.mpropr00.dscrpr}"
														filterBy="#{tqigqg00.mpropr00.dscrpr}}" filterMatchMode="contains"  width="150">
												<h:outputText value="#{tqigqg00.mpropr00.dscrpr}" />
											</p:column>
					
					
											<!--Moneda -->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('idmoqg')}"
												filterBy="#{tqigqg00.mgenus001.codius}" filterMatchMode="contains"
												width="120" sortBy="#{tqigqg00.mgenus001.codius}">
												<h:outputText value="#{tqigqg00.mgenus001.codius}-#{tqigqg00.mgenus001.dcttus}" />
											</p:column>
											
											
											<!--Valor Cotización -->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('vacoqg')}" sortBy="#{tqigqg00.vacoqg}"
														filterBy="#{tqigqg00.vacoqg}" filterMatchMode="contains"  width="120">
												<h:outputText value="#{tqigqg00.vacoqg}">
													<pe:keyFilter regEx="/[\d\-\.]/" />		
													<f:convertNumber pattern="#{seTqigqg00.paternDec_CantiM}" locale="en_US"/>
												</h:outputText>
											</p:column>	
											
											<!--Estado-->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('estado')}"
												filterBy="#{tqigqg00.tqesqeAux.mestad00.dscres}" filterMatchMode="contains"
												width="120" sortBy="#{tqigqg00.tqesqeAux.mestad00.dscres}">
												<h:outputText value="#{tqigqg00.tqesqeAux.mestad00.dscres}" />
											</p:column>
											 
					
											<!-- Registro activo -->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('regcqg')}"
												filterBy="#{tqigqg00.regcqg}" filterMatchMode="contains"
												width="120" exportable="false">
												<f:facet name="filter">
													<p:selectOneButton onchange="PF('fTable').filter()">
														<f:converter converterId="javax.faces.Boolean" />
														<f:selectItem itemLabel="Todo" itemValue="" />
														<f:selectItem itemLabel="S" itemValue="true" />
														<f:selectItem itemLabel="N" itemValue="false" />
													</p:selectOneButton>
												</f:facet>
												<h:outputText
													value="#{tqigqg00.regcqg ? seTemplate.rb.getString('mj_activo_si') : seTemplate.rb.getString('mj_activo_no')}" />
											</p:column>
										</p:dataTable>
									</h:form>
									
									<!-- ///////////////////////////////////////////////////////////// -->
									<!-- ////////////////// CARGA DE ARCHIVOS COTIZACION////////////// -->
									<ui:include src="/pages/transacciones/detalles/pacdTqdrqr00.xhtml" />							
									<ui:include src="/pages/transacciones/detalles/pacdTqdfqf00.xhtml" />
									<ui:include src="/pages/transacciones/detalles/pacdTqdrfg00.xhtml" />
									
									

								
								<!-- //////////////////////////////////////////////////////////////////////// -->
								<!-- /////////////////////// ACTUALIZAR - MODIFICAR ///////////////////////// -->
								
								<h:form id="formDetalle">
								
					
									<ui:include src="/pages/listas/palmconca.xhtml" />
									<ui:include src="/pages/listas/palmgenteCliente.xhtml" />
									<ui:include src="/pages/listas/palmgenteSigla.xhtml" />
									<ui:include src="/pages/listas/palMgenusOrganiVentas.xhtml" />
									<ui:include src="/pages/listas/palMgenusCanalDistri.xhtml" />
									<ui:include src="/pages/listas/palMfunfu.xhtml" />
									<ui:include src="/pages/listas/palmpropr.xhtml" />
									<ui:include src="/pages/listas/palmtiptx.xhtml" />
									<ui:include src="/pages/listas/palmgenusTipoNaturaleza.xhtml" /> 
									<ui:include src="/pages/listas/palMgenusMonedaCot.xhtml" /> 
									
									
									
									
									<p:dialog header="#{seTqigqg00.titulo}" id="dialogoTqigqg00" widgetVar="dlgNuevoUpdate" resizable="false" modal="true" responsive="true"
											   height="570" width="#{seTemplate.MAX_WIDTH_DIALOG}"  minimizable="true" >
											  
											  
											  
										<p:ajax event="close" listener="#{seTqigqg00.evtCloseDialogTqigqg00}" />
					
										<p:messages id="messagesDetalle" showDetail="false" autoUpdate="true" closable="true" />
										
										
										<p:tabView id="tabView" widgetVar="wtabView" activeIndex="#{seTqigqg00.tabView}">
										<p:messages id="messages2" showDetail="false" autoUpdate="true" closable="true" />
										<p:ajax event="tabChange" update=":formDetalle:tabView:pnlDetalle,:formDetalle:tabView:pnlCabecera3,:formDetalle:tabView:pnlDetalle2" listener="#{seTqigqg00.onTabChange}" />
										
											<p:tab title="#{seTqigqg00.idiomasHm.get('infgrl')}" id="tabInfoGrnl" widgetVar="tabInfoGrnl" rendered="#{seTqigqg00.tabInfBase}">
												<h:panelGrid id="pnlDetalle" columns="1" styleClass="ui-panelgrid-blank" >
													<p:panelGrid id="pnlTqigqg00" styleClass="ui-panelgrid-blank" >
													
														<p:row>														
														<!-- SIGLA-->
															<p:column>
																<h:outputLabel for="siglacColumnDesc" value="#{seTqigqg00.idiomasHm.get('siglac')}" />
															</p:column>
															<p:column>
																<p:autoComplete id="siglacColumn" readonly="#{seTqigqg00.botonReabrir}"
																	value="#{seTqigqg00.tqigqg00.mgente00}" var="p"
																	itemLabel="#{p.codite}" itemValue="#{p}"
																	converter="#{mgente00Converter}"
																	completeMethod="#{seMgente00.completeMgente}" maxlenght="20"
																	size="20" forceSelection="true" scrollHeight="250"
																	placeholder="#{mensajes.placeholder_buscar}"
																	label="#{seTqigqg00.idiomasHm.get('siglac')}" 
																	requiredMessage="#{seTqigqg00.idiomasHm.get('siglac')} : #{seTqigqg00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
																	<p:ajax event="itemSelect" update="siglacColumnDesc,cliente1,cliente2" />
																	<p:ajax event="itemSelect" listener="#{seMgente00.mostrarDatos}"/>
																	<f:attribute name="codtte" value="#{params.mgente_codtte_cliente}" />
																	<p:ajax event="blur" update="siglacColumnDesc,cliente1,cliente2" />
																</p:autoComplete>
																	<p:inputText id="siglacColumnDesc" value="#{seTqigqg00.tqigqg00.mgente00.aliate}" readonly="true"  binding="#{seTqigqg00.inputTextSiglacColumn}"/>
																<p:commandButton id="mgenteBtnSigla" icon="ui-icon-search" oncomplete="PF('dialogoMgenteSigla').show();" rendered ="#{!(seTqigqg00.botonReabrir)}"
																	update=":formDetalle:dTableMgenteSigla" process="@this">
																	<f:setPropertyActionListener target="#{seMgente00.objSeCall}" value="#{seTqigqg00}" />
																	<f:setPropertyActionListener target="#{seMgente00.nombreTabla}" value="#{seMgente00.tabla}" />
																	<f:setPropertyActionListener target="#{seMgente00.nombreLista}" value="sigla" />
																	<f:setPropertyActionListener target="#{seMgente00.update}" value=":formDetalle:tabView:cliente1,:formDetalle:tabView:cliente2,:formDetalle:tabView:siglacColumn, :formDetalle:tabView:siglacColumnDesc" />
																</p:commandButton>
															</p:column>
															

																<p:column>
																	<h:outputLabel  for="cliente1" value="#{seTqigqg00.idiomasHm.get('idclqg')}"/>
																</p:column>
																<p:column>
																	<p:inputText id="cliente1"  readonly="true" value="#{seTqigqg00.tqigqg00.mgente00.prnmte} #{seTqigqg00.tqigqg00.mgente00.prapte}" rendered="#{seMgente00.perNatural}" />
																	<p:inputText id="cliente2"  readonly="true" value="#{seTqigqg00.tqigqg00.mgente00.dctate}" rendered="#{seMgente00.perJuridica}"/>
																</p:column>
															
															</p:row>

														
														<p:row>
															<!-- Numero Cotización -->
															<p:column>
																<h:outputLabel for="nroqqgColumn" readonly="#{seTqigqg00.flagAnulada}" value="#{seTqigqg00.idiomasHm.get('nroqqg')}" />
															</p:column>
															<p:column>
							
																<p:inputText id="nroqqgColumn" readonly="true" value="#{seTqigqg00.tqigqg00.mgente00.aliate}#{seTqigqg00.fechaActual}#{seTqigqg00.sufijo}" rendered="#{seTqigqg00.numNuevo}"
																	label="#{seTqigqg00.idiomasHm.get('nroqqg')}"
																	required="#{seTqigqg00.permisoCampos.get('nroqqg').reqcam.equals('S')}"
																	requiredMessage="#{seTqigqg00.idiomasHm.get('nroqqg')} : #{seTqigqg00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
																	<f:ajax event="blur" update=":formDetalle:tabView:nroqqgColumn" />																	
																</p:inputText>
																
																<p:inputText id="nroqqgColumnRender" readonly="true" value="#{seTqigqg00.tqigqg00.nroqqg}" rendered="#{seTqigqg00.numModificar}"
																	label="#{seTqigqg00.idiomasHm.get('nroqqg')}"
																	required="#{seTqigqg00.permisoCampos.get('nroqqg').reqcam.equals('S')}"
																	requiredMessage="#{seTqigqg00.idiomasHm.get('nroqqg')} : #{seTqigqg00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
																	<f:ajax event="blur" update=":formDetalle:tabView:nroqqgColumnRender" />																	
																</p:inputText>
																
																
															</p:column>
															
																<!-- estado -->

															<p:column>
																	<h:outputLabel for="estadoColumn" value="#{seTqigqg00.idiomasHm.get('estado')}:  " />
															</p:column>
															<p:column>
																<h:outputText id="estadoColumn" value="#{seTqigqg00.descEstado}" />
															</p:column>
															
															
															
															
														</p:row>
														<p:row>
														
															<!-- COMPAÑIA -->
															    <p:column>
																	<h:outputLabel  for="compac" value="#{seTqigqg00.idiomasHm.get('compac')}"/>
																</p:column>
																<p:column>
																	<p:inputText id="compac"  readonly="true" value="#{seTqigqg00.compac}" />
																</p:column>
													    </p:row>
														
															
														<!-- Id tipo transacción FK MTIPTX00-->
														<p:row>
															    <p:column>
																	<h:outputLabel  for="idtxqgColumn" value="#{seTqigqg00.idiomasHm.get('idtxqg')}"/>
																</p:column>
																<p:column>
																	<p:inputText id="idtxqgColumn"  readonly="true" value="#{seTqigqg00.dstiptx}" rendered="#{seTqigqg00.numNuevo}"/>
																	<p:inputText id="idtxqgColumn2"  readonly="true" value="#{seTqigqg00.tqigqg00.mtiptx00.dstitx}" rendered="#{seTqigqg00.numModificar}"/>
																</p:column>
															
														</p:row>
														
							
														
														
														
													
														<!--Moneda FK a MGENUS001 -->
														<p:row>
															<p:column>
																<h:outputLabel for="idmoqgColumnDesc" value="#{seTqigqg00.idiomasHm.get('idmoqg')}" />
															</p:column>
															<p:column>										
																	<p:autoComplete id="idmoqgColumn" value="#{seTqigqg00.tqigqg00.mgenus001}" readonly="#{seTqigqg00.botonReabrir}"
																			var="p" itemLabel="#{p.codius}" itemValue="#{p}"
																			converter="#{mgenus00Converter}" completeMethod="#{seMgenus00.completeMgenus}"
																			maxlenght="20" size="20"  forceSelection="true" scrollHeight="250"
																			placeholder="#{mensajes.placeholder_buscar}"
																			label="#{seTqigqg00.idiomasHm.get('idmoqg')}"
																			requiredMessage="#{seTqigqg00.idiomasHm.get('idmoqg')} : #{seTqigqg00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">														
																			<p:ajax event="itemSelect" update="idmoqgColumnDesc" />
																			<p:ajax event="itemSelect" listener="#{seMgenus00.mostrarDatos}"/>
																			<f:attribute name="codtus" value="#{params.mgenus_codtus_Cotizacion}" />
																			<p:ajax event="blur" update="idmoqgColumnDesc" />
																	</p:autoComplete>
																	<p:inputText id="idmoqgColumnDesc" value="#{seTqigqg00.tqigqg00.mgenus001.dcttus}" readonly="true" binding="#{seTqigqg00.inputTextIdmoqgColumn}"/>
																<p:commandButton id="mgenusBtnMoneda" icon="ui-icon-search" rendered ="#{!(seTqigqg00.botonReabrir)}"
																	oncomplete="PF('dlgMgenus00Moneda').show();"
																	update=":formDetalle:dTableMgenus00Moneda" process="@this">
																	<f:setPropertyActionListener target="#{seMgenus00.objSeCall}" value="#{seTqigqg00}" />
																	<f:setPropertyActionListener target="#{seMgenus00.nombreTabla}" value="#{seMgenus00.tabla}" />
																	<f:setPropertyActionListener target="#{seMgenus00.nombreLista}"	value="moneda" />
																	<f:setPropertyActionListener target="#{seMgenus00.update}" 	value=":formDetalle:tabView:idmoqgColumn, :formDetalle:tabView:idmoqgColumnDesc" />
																</p:commandButton>
															</p:column>
														</p:row>
									
																			
														<!-- Producto FK a MPROPR00  -->
									
														<p:row>
															<p:column>
																<h:outputLabel for="idppqgColumnDesc" value="#{seTqigqg00.idiomasHm.get('idppqg')}" />
															</p:column>
															<p:column>
																<p:autoComplete id="idppqgColumn" readonly="#{seTqigqg00.botonReabrir}"
																	value="#{seTqigqg00.tqigqg00.mpropr00}" var="p"
																	itemLabel="#{p.codcpr}" itemValue="#{p}"
																	converter="#{mpropr00Converter}"
																	completeMethod="#{seMpropr00.completeMpropr}" maxlenght="20"
																	size="20" forceSelection="true" scrollHeight="250"
																	placeholder="#{mensajes.placeholder_buscar}"
																	label="#{seTqigqg00.idiomasHm.get('idppqg')}"
																	requiredMessage="#{seTqigqg00.idiomasHm.get('idppqg')} : #{seTqigqg00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}" >
																	<p:ajax event="itemSelect" update="idppqgColumnDesc" />
																	<p:ajax event="blur" update="idppqgColumnDesc" />
																</p:autoComplete>
																	<p:inputText id="idppqgColumnDesc" value="#{seTqigqg00.tqigqg00.mpropr00.dscrpr}" readonly="true"  binding="#{seTqigqg00.inputTextIdppqgColumn}"/>
																<p:commandButton id="mproprBtnProdu" icon="ui-icon-search" oncomplete="PF('dialogoMpropr').show();" rendered ="#{!(seTqigqg00.botonReabrir)}"
																	update=":formDetalle:dTableMpropr" process="@this">
																	<f:setPropertyActionListener target="#{seMpropr00.objSeCall}" value="#{seTqigqg00}" />
																	<f:setPropertyActionListener target="#{seMpropr00.nombreTabla}" value="#{seMpropr00.tabla}" />
																	<f:setPropertyActionListener target="#{seMpropr00.nombreLista}" value="producto" />
																	<f:setPropertyActionListener target="#{seMpropr00.update}" value=":formDetalle:tabView:idppqgColumn, :formDetalle:tabView:idppqgColumnDesc" />
																</p:commandButton>
															</p:column>
															

															<!--Valor Cotizacion -->
															<p:column >
																<h:outputLabel for="vacoqgColumn" value="#{seTqigqg00.idiomasHm.get('vacoqg')}" />
															</p:column>
															<p:column>

																<p:inputText id="vacoqgColumn" readonly="#{seTqigqg00.botonReabrir}" value="#{seTqigqg00.tqigqg00.vacoqg}"
																	label="#{seTqigqg00.idiomasHm.get('vacoqg')}">
																	<pe:keyFilter regEx="/[\d\-\.]/" />		
																    <f:convertNumber pattern="#{seTqigqg00.paternDec_CantiM}" locale="en_US"/>	
																    <p:ajax event="blur" process="@this" update="vacoqgColumn" />								
															   </p:inputText>
																
															</p:column>	
															
															
														</p:row>
														
														
															<!--  Organización ventas Default para el Tercero  FK a MGENUS002 -->
														<p:row>
															<p:column>
																<h:outputLabel for="idovqgColumnDesc" value="#{seTqigqg00.idiomasHm.get('idovqg')}" />
															</p:column>
