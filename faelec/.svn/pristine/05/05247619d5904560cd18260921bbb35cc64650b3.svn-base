<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN" "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<f:view xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui">
	<html>
						
			<p:commandLink oncomplete="PF('dlgNuevoUpdateMgentd00').show();" update=":formMgentd"
						   actionListener="#{seMgentd00.accionNuevo}" process="@this">
				 <p:graphicImage value="/resources/images/nuevo_ico2.ico" style="border:0;margin-right:5px;" height="25" />
			</p:commandLink>

				
				<!-- //////////////////////////////////////////////////////////////////////// -->
				<!-- //////////////////////////////// DATATABLE ///////////////////////////// -->
				<p:messages id="messages4" showDetail="false" autoUpdate="true" closable="true" />
				
				<p:dataTable id="dTableMgentd00" widgetVar="fTableMgentd00" reflow="true"
					var="mgentd00" paginator="true" paginatorPosition="bottom" rows="10"
					rowKey="#{mgentd00.idtrtd}#{mgentd00.mgente00.idtrte}#{mgentd00.mgenus001.idtrus}#{mgentd00.mgenus002.idtrus}#{mgentd00.mgenus003.idtrus}" 	
					emptyMessage="#{mensajes.grid_emptyMessage}"
					value="#{seMgente00.mgente00.mgentd00s}" selectionMode="single"
					selection="#{seMgentd00.mgentd00}"
					filteredValue="#{seMgentd00.filteredMgentd00}"
					resizableColumns="true" scrollable="true"
					currentPageReportTemplate="#{mensajes.grid_total_registros} {totalRecords}"
					paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink}">
				
						<p:ajax event="rowSelect" listener="#{seMgentd00.accionModificar}"
									oncomplete="PF('dlgNuevoUpdateMgentd00').show();" update=":formMgentd" />
						
						<!-- Columnas Limpiar filtros y Eliminar -->
								<p:column style="width:10px;" exportable="false">
								<f:facet name="header">
									<p:commandLink id="clarFilterLink"	onclick="PF('fTableMgentd00').clearFilters();" actionListener="#{seMgentd00.clearAllFilters}">
										<p:graphicImage value="/resources/images/clean_filters.png"  height="16" 
										title="#{mensajes.boton_limpiar_filtros}" />
									</p:commandLink>
								</f:facet>
								<p:commandLink id="deleteLink" update=":formDetalle:tabView:dTableMgentd00" actionListener="#{seMgentd00.remove}" process="@this">
									<p:graphicImage value="/resources/images/Trash.gif" style="border:0;" height="16" title="#{mensajes.boton_eliminar}" />
										<p:confirm header="#{mensajes.confirm_titulo}" 	message="#{mensajes.confirm_mensaje_remove}" icon="ui-icon-alert" />
										<f:attribute name="mgentd00" value="#{mgentd00}" />
									</p:commandLink>
								</p:column>
						
						<!-- Pais -->
						<p:column headerText="#{seMgentd00.idiomasHm.get('idpatd')}"
							filterBy="#{mgentd00.mgenus002.codius} #{mgentd00.mgenus002.dcttus}" filterMatchMode="contains" width="120"
							sortBy="#{mgentd00.mgenus002.codius} #{mgentd00.mgenus002.dcttus}">
							<h:outputText value="#{mgentd00.mgenus002.codius} #{mgentd00.mgenus002.dcttus}" />
						</p:column>
						
						<!-- Departamento -->
						<p:column headerText="#{seMgentd00.idiomasHm.get('iddptd')}"
							filterBy="#{mgentd00.mgenus003.codius} #{mgentd00.mgenus003.dcttus}" filterMatchMode="contains" width="120"
							sortBy="#{mgentd00.mgenus003.codius} #{mgentd00.mgenus003.dcttus}">
							<h:outputText value="#{mgentd00.mgenus003.codius} #{mgentd00.mgenus003.dcttus}" />
						</p:column>
						
						<!-- Ciudad -->
						<p:column headerText="#{seMgentd00.idiomasHm.get('idcitd')}"
							filterBy="#{mgentd00.mgenus001.codius} #{mgentd00.mgenus001.dcttus}" filterMatchMode="contains" width="120"
							sortBy="#{mgentd00.mgenus001.codius} #{mgentd00.mgenus001.dcttus}">
							<h:outputText value="#{mgentd00.mgenus001.codius} #{mgentd00.mgenus001.dcttus}" />
						</p:column>
						
						<!--Direccion-->
							<p:column headerText="#{seMgentd00.idiomasHm.get('diretd')}" filterMatchMode="contains" width="120"
							sortBy="#{mgentd00.diretd}" filterBy="#{mgentd00.diretd}"  >
							<h:outputText value="#{mgentd00.diretd}"  />
						</p:column>																
						
						<!-- Registro activo -->
						<p:column headerText="#{seMgentd00.idiomasHm.get('regitd')}"
							filterBy="#{mgentd00.regitd}" filterMatchMode="contains" width="100">
							<f:facet name="filter">
								<p:selectOneButton onchange="PF('fTableMgentd00').filter()">
									<f:converter converterId="javax.faces.Boolean" />
									<f:selectItem itemLabel="Todo" itemValue="" />
									<f:selectItem itemLabel="S" itemValue="true" />
									<f:selectItem itemLabel="N" itemValue="false" />
								</p:selectOneButton>
							</f:facet>
							<h:outputText value="#{mgentd00.regitd ? seTemplate.rb.getString('mj_activo_si') : seTemplate.rb.getString('mj_activo_no')}" />
						</p:column>
										
			</p:dataTable>								

	</html>
</f:view>