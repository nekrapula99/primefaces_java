																	<f:attribute name="codtus" value="#{params.mgenus_codtus_lineaProductos}" />
																	<p:ajax event="blur" update="idliprColumnDesc" />
															</p:autoComplete>
															<p:inputText id="idliprColumnDesc" value="#{seMpropr00.mpropr00.mgenus002.dcttus}" readonly="true"/>
																<p:commandButton id="mproprBtnLin" icon="ui-icon-search" oncomplete="PF('dialogoMgenusLineaProducto').show();"
																	update=":formDetalle:dTableMgenusLineaProducto" process="@this">
																	<f:setPropertyActionListener target="#{seMgenus00.objSeCall}" value="#{seMpropr00}" />
																	<f:setPropertyActionListener target="#{seMgenus00.nombreTabla}" value="#{seMgenus00.tabla}" />
																	<f:setPropertyActionListener 	target="#{seMgenus00.nombreLista}" value="lineaProductos" />
																	<f:setPropertyActionListener target="#{seMgenus00.update}" value=":formDetalle:tabView:idliprColumn, :formDetalle:tabView:idliprColumnDesc" />
																</p:commandButton>
															</p:column>
														</p:row>
					
					
														<!-- Sublinea producto FK IDSLPR -->
