																	<f:setPropertyActionListener target="#{seMconca00.nombreTabla}" value="#{seMconca00.tabla}" />
																	<f:setPropertyActionListener target="#{seMconca00.nombreLista}" value="companias" />
																	<f:setPropertyActionListener target="#{seMconca00.update}" value=":formDetalle:tabView:idcmprColumn, :formDetalle:tabView:idcmprColumnDesc" />
																</p:commandButton>
															</p:column>
														</p:row>
					
														<!--Descripcion-->
