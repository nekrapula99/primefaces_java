															<p:column colspan="2">
																<p:inputTextarea id="dscrprColumn"  value="#{seMpropr00.mpropr00.dscrpr}" 
																	label="#{seMpropr00.idiomasHm.get('dscrpr')}" maxlength="1000" rows="3" cols="52" >
																	<f:ajax event="blur" update=":formDetalle:tabView:idcmprColumnDesc2" />
																</p:inputTextarea>
															</p:column>
														</p:row>
					
														<!-- Marca Comercial FK IDMRPR -->
														<p:row>
															<p:column>
																<h:outputLabel for="idmrprColumn" value="#{seMpropr00.idiomasHm.get('idmrpr')}" />
															</p:column>
															<p:column>
																<p:autoComplete id="idmrprColumn" value="#{seMpropr00.mpropr00.mgenus001}" 
