<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:p="http://primefaces.org/ui"
                template="/WEB-INF/template.xhtml"
                xmlns:pe="http://primefaces.org/ui/extensions">

    <ui:define name="content">
    
        <div class="ui-g">
        
            <div class="ui-g-12">
                  
                <div class="card">
                	<h1>#{seMpropr00.titulo}</h1>
					
									<!-- //////////////////////////////////////////////////////////////////////////////// -->
									<!-- //////////////////////////////CABECERA////////////////////////////////////////// -->
									<h:form id="formCabecera">
										<p:remoteCommand autoRun="true">
											<f:setPropertyActionListener target="#{seCabecera.objCall}" value="#{seMpropr00}" />
											<f:setPropertyActionListener target="#{seCabecera.varAdicional}" value="#{mpropr00}" />
											<f:setPropertyActionListener target="#{seCabecera.pac}" value="pacMpropr00" />
											<f:setPropertyActionListener target="#{seCabecera.seControl.titulo}" value="#{seMpropr00.titulo}" />
											<f:event type="preRenderView" listener="#{seMpropr00.preRenderView}" />
										</p:remoteCommand>
										<ui:include src="/pages/plantillas/cabecera.xhtml" />
									</h:form>
					
									<h:form id="formNuevoUpdate">
										<p:remoteCommand name="rcNuevoUpdate"
											oncomplete="PF('dlgNuevoUpdate').show();"
											actionListener="#{seMpropr00.accionNuevo}" update=":formDetalle" />
									</h:form>
					
									<!-- //////////////////////////////////////////////////////////////////////// -->
									<!-- //////////////////////////////// DATATABLE ///////////////////////////// -->
									<h:form id="formTable">
										<p:messages id="messages" showDetail="false" autoUpdate="true" closable="true" />
					
										<p:dataTable id="dTable" widgetVar="fTable" var="mpropr00" reflow="true"
											paginator="true" paginatorPosition="bottom" rows="10"
											rowKey="#{mpropr00.idcapr}" scrollable="true"
											emptyMessage="#{mensajes.grid_emptyMessage}"
											value="#{seMpropr00.listMpropr00}" selectionMode="single"
											selection="#{seMpropr00.mpropr00}"
											filteredValue="#{seMpropr00.filteredMpropr00}"
											resizableColumns="true"
											currentPageReportTemplate="#{mensajes.grid_total_registros} {totalRecords}"
											paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink}">
					
											<p:ajax disabled="#{seCabecera.seControl.PERMISOSACCION.get('BTN_ACTUALIZAR')==null}" event="rowSelect" listener="#{seMpropr00.accionModificar}"
												oncomplete="PF('dlgNuevoUpdate').show();" update=":formDetalle" />
					
											<p:column style="width:14px;" exportable="false">
												<f:facet name="header">
													<p:commandLink id="clarFilterLink"	onclick="PF('fTable').clearFilters();"  actionListener="#{seMpropr00.clearAllFilters}">
													<p:graphicImage value="/resources/images/clean_filters.png"  height="16" title="#{mensajes.boton_limpiar_filtros}" />
													</p:commandLink>
												</f:facet>
												<p:commandLink disabled="#{seCabecera.seControl.PERMISOSACCION.get('BTN_ELIMINAR')==null}" id="deleteLink" update=":formTable" actionListener="#{seMpropr00.remove}" process="@this">
													<p:graphicImage value="/resources/images/Trash.gif"	style="border:0;" height="16" title="#{mensajes.boton_eliminar}" />
													<p:confirm header="#{mensajes.confirm_titulo}" message="#{mensajes.confirm_mensaje_remove}" icon="ui-icon-alert" />
													<f:attribute name="mpropr00" value="#{mpropr00}" />
												</p:commandLink>
											</p:column>
					
											<!-- Codigo producto -->
											<p:column headerText="#{seMpropr00.idiomasHm.get('codcpr')}"
												filterBy="#{mpropr00.codcpr}" filterMatchMode="contains"
												width="100" sortBy="#{mpropr00.codcpr}">
												<h:outputText value="#{mpropr00.codcpr}" />
											</p:column>
					
											<!-- Codigo Compañia -->
											<p:column headerText="#{seMpropr00.idiomasHm.get('idcmpr')}" sortBy="#{mpropr00.idcmpr}" width="100"
												filterBy="#{mpropr00.mconca00.codcia}-#{mpropr00.mconca00.nomcia}" filterMatchMode="contains">
												<h:outputText value="#{mpropr00.mconca00.codcia}-#{mpropr00.mconca00.nomcia}" />
											</p:column>
					
											<!--Descripcion-->
											<p:column headerText="#{seMpropr00.idiomasHm.get('dscrpr')}"
												filterBy="#{mpropr00.dscrpr}" filterMatchMode="contains"
												width="100" sortBy="#{mpropr00.dscrpr}">
												<h:outputText value="#{mpropr00.dscrpr}" />
											</p:column>
					
					
											<!-- Linea -->
											<p:column headerText="#{seMpropr00.idiomasHm.get('idlipr')}"
												filterBy="#{mpropr00.mgenus002.codius}" filterMatchMode="contains"
												width="100" sortBy="#{mpropr00.mgenus002.codius}">
												<h:outputText value="#{mpropr00.mgenus002.codius}-#{mpropr00.mgenus002.dcttus}"/>
											</p:column>
					
											<!--Sublinea -->
											<p:column headerText="#{seMpropr00.idiomasHm.get('idslpr')}"
												filterBy="#{mpropr00.mgenus003.codius}" filterMatchMode="contains"
												width="120" sortBy="#{mpropr00.mgenus003.codius}">
												<h:outputText value="#{mpropr00.mgenus003.codius}-#{mpropr00.mgenus003.dcttus}" />
											</p:column>
					
											<!-- Registro activo -->
											<p:column headerText="#{seMpropr00.idiomasHm.get('regipr')}"
												filterBy="#{mpropr00.regipr}" filterMatchMode="contains"
												width="120" exportable="false">
												<f:facet name="filter">
													<p:selectOneButton onchange="PF('fTable').filter()">
														<f:converter converterId="javax.faces.Boolean" />
														<f:selectItem itemLabel="Todo" itemValue="" />
														<f:selectItem itemLabel="S" itemValue="true" />
														<f:selectItem itemLabel="N" itemValue="false" />
													</p:selectOneButton>
												</f:facet>
												<h:outputText
													value="#{mpropr00.regipr ? seTemplate.rb.getString('mj_activo_si') : seTemplate.rb.getString('mj_activo_no')}" />
											</p:column>
										</p:dataTable>
									</h:form>
								
									<!-- Dialogo descripcion otros idiomas -->
								<h:form id="formMdespr00Detalle">		
									<ui:include src="/pages/listas/palmtiptx.xhtml" />
									<ui:include src="/pages/listas/palmidiom.xhtml" />
									
									<p:dialog header="#{seMpropr00.idiomasHm.get('desoid')}" widgetVar="dlgNuevoUpdateMdespr00"
										      height="#{seTemplate.MAX_HEIGHT_DIALOG}" width="#{seTemplate.MAX_WIDTH_DIALOG}" 
											  resizable="false" modal="true" responsive="true">
										
										<p:ajax event="close" listener="#{seMdespr00.evtCloseDialog}" />
															
										<h:panelGrid id="pnlDetalleMdespr00" columns="1" styleClass="ui-panelgrid-blank" >
											<p:messages id="messagesDialogMdespr00" showDetail="false"
												autoUpdate="true" closable="true" />
					
											<p:panelGrid id="pnlMdespr00Dialog" styleClass="ui-panelgrid-blank">
					
												<!-- Id tipo transacción FK MTIPTX00-->
												<p:row>
													<p:column>
														<h:outputLabel for="idttdpColumn" value="#{seMdespr00.idiomasHm.get('idttdp')}" />
													</p:column>
													<p:column>
														<p:autoComplete id="idttdpColumn"
															value="#{seMdespr00.mdespr00.mtiptx00}" var="p"
															itemLabel="#{p.idtitx}" itemValue="#{p}"
															converter="#{mtiptx00Converter}" 
															completeMethod="#{seMtiptx00.completeMtiptx00}" maxlenght="20"
															size="20" forceSelection="true" scrollHeight="250"
															placeholder="#{mensajes.placeholder_buscar}"
															label="#{seMdespr00.idiomasHm.get('idttdp')}" >
															<p:ajax event="itemSelect" update="idttdpColumnDesc, transaccion" />
															<p:ajax event="blur" update="idttdpColumnDesc, transaccion" />
														</p:autoComplete>
														<p:inputText id="idttdpColumnDesc" value="#{seMdespr00.mdespr00.mtiptx00.dstitx}" readonly="true" />
														<p:commandButton id="idttdpBtn" icon="ui-icon-search" oncomplete="PF('dlgMtiptx00').show();"
															update=":formMdespr00Detalle:dTableMtiptx00" process="@this">
															<f:setPropertyActionListener target="#{seMtiptx00.objSeCall}" value="#{seMdespr00}" />
															<f:setPropertyActionListener target="#{seMtiptx00.nombreTabla}" value="#{seMtiptx00.tabla}" />
															<f:setPropertyActionListener target="#{seMtiptx00.nombreLista}" value="transaccion" />
															<f:setPropertyActionListener target="#{seMtiptx00.update}" value="formMdespr00Detalle:transaccion, formMdespr00Detalle:idttdpColumn, formMdespr00Detalle:idttdpColumnDesc" /> 
														</p:commandButton>
													</p:column>
												</p:row>	
												
												<!-- transaccion -->
												<p:row>				
													<p:column>
														<h:outputLabel  value="Transaccion" />
													</p:column>
													<p:column>
														<p:inputText id="transaccion"  readonly="true" value="#{seMdespr00.mdespr00.mtiptx00.mgencg00.codicg} #{seMdespr00.mdespr00.mtiptx00.mgencg00.dcttcg}" />
													</p:column>
												</p:row>
												
												<!-- Codigo Idioma -->
												<p:row>
													<p:column>
														<h:outputLabel for="ididdpColumn" value="#{seMdespr00.idiomasHm.get('ididdp')}" />
													</p:column>
													<p:column>
														<p:autoComplete id="ididdpColumn" value="#{seMdespr00.mdespr00.midiom00}" 
																	var="p" itemLabel="#{p.codidi}" itemValue="#{p}"
																	converter="#{midiom00Converter}" completeMethod="#{seMidiom00.completeMidiom}"
																	maxlenght="20" size="20"  forceSelection="true" scrollHeight="250"
																	placeholder="#{mensajes.placeholder_buscar}" >								
																	<p:ajax event="itemSelect" update="ididdpColumnDesc" />
																	<p:ajax event="blur" update="ididdpColumnDesc" />
														</p:autoComplete>
														<p:inputText id="ididdpColumnDesc" value="#{seMdespr00.mdespr00.midiom00.nomidi}" readonly="true"
																	 label="#{seMdespr00.idiomasHm.get('ididdp')}"/>
														<p:commandButton id="midiomBtn" icon="ui-icon-search" 	oncomplete="PF('dialogoMidiom').show();"
															update=":formMdespr00Detalle:dTableMidiom00" process="@this">
															<f:setPropertyActionListener target="#{seMidiom00.objSeCall}" value="#{seMdespr00}" />
															<f:setPropertyActionListener target="#{seMidiom00.nombreTabla}" value="#{seMidiom00.tabla}" />
															<f:setPropertyActionListener target="#{seMidiom00.nombreLista}" value="idiomas" />
															<f:setPropertyActionListener target="#{seMidiom00.update}" value=":formMdespr00Detalle:ididdpColumn,:formMdespr00Detalle:ididdpColumnDesc" />
														</p:commandButton>
													</p:column>
												</p:row>
					
												<p:row>
													<!-- Descripcion -->
													<p:column>
														<h:outputLabel for="dscrdpColumn" value="#{seMdespr00.idiomasHm.get('dscrdp')}" />
													</p:column>
													<p:column>
														<p:inputTextarea id="dscrdpColumn" maxlength="1000" rows="5" cols="52" value="#{seMdespr00.mdespr00.dscrdp}" 
															label="#{seMdespr00.idiomasHm.get('dscrdp')}" />
													</p:column>
												</p:row>
					
												<p:row>
													<!-- Registro activo -->
													<p:column>
														<h:outputLabel for="regidpColumn" value="#{seMdespr00.idiomasHm.get('regidp')}" />
													</p:column>
													<p:column>
														<p:selectBooleanCheckbox id="regidpColumn" label="#{seMdespr00.idiomasHm.get('regidp')}"
															value="#{seMdespr00.mdespr00.regidp}" required="true" />
													</p:column>
												</p:row>
											</p:panelGrid>
										</h:panelGrid>
					
										<f:facet name="footer">
											<p:commandButton value="#{mensajes.boton_guardar}"
												actionListener="#{seMdespr00.save}" 
												update=":formMdespr00Detalle:pnlDetalleMdespr00,:formDetalle:tabView:dTableMdespr00">
												<p:confirm header="#{mensajes.confirm_titulo}" message="#{mensajes.confirm_mensaje_save}" icon="ui-icon-alert" />
											</p:commandButton>
										</f:facet>
					
									</p:dialog>
								</h:form>		
								
								
								<!-- //////////////////////////////////////////////////////////////////////// -->
								<!-- ///////////////// ACTUALIZAR - MODIFICAR (MVARPR00) //////////////////// -->	
								<ui:include src="/pages/maestros/detalles/pacdMvarpr00.xhtml" />	
					
								<!-- //////////////////////////////////////////////////////////////////////// -->
								<!-- /////////////////////// ACTUALIZAR - MODIFICAR ///////////////////////// -->
					
								<!-- 	DIALOG Mpropr00 -->
								<h:form id="formDetalle">
					
									<ui:include src="/pages/listas/palmconca.xhtml" />
									<ui:include src="/pages/listas/palmgenusMarcaComercial.xhtml" />
									<ui:include src="/pages/listas/palmgenusLineaProducto.xhtml" />
									<ui:include src="/pages/listas/palmgenusSubLineaProducto.xhtml" />
									<ui:include src="/pages/listas/palmgenusUnidadMedida.xhtml" />
									<ui:include src="/pages/listas/palmgenusCentroCostos.xhtml" />
									<ui:include src="/pages/listas/palMgenus00Variantes.xhtml" />
									<ui:include src="/pages/listas/palMgenusUniMedidaIng.xhtml" />
					
									<p:dialog header="#{seMpropr00.titulo}" widgetVar="dlgNuevoUpdate" resizable="false" modal="true" responsive="true"
											  height="#{seTemplate.MAX_HEIGHT_DIALOG}" width="#{seTemplate.MAX_WIDTH_DIALOG}"  >
											  
										<p:ajax event="close" listener="#{seMpropr00.evtCloseDialogMpropr}" />
					
										<p:messages id="messagesDetalle" showDetail="false" autoUpdate="true" closable="true" />
										
										<p:tabView id="tabView" widgetVar="tabViewRamiro" activeIndex="#{seMpropr00.tabView}" dynamic="true">
