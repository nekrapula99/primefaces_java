																		<f:attribute name="codtus" value="#{params.mgenus_codtus_marcaComercial}" />
																		<p:ajax event="blur" update="idmrprColumnDesc" />
																</p:autoComplete>
															<p:inputText id="idmrprColumnDesc" value="#{seMpropr00.mpropr00.mgenus001.dcttus}" readonly="true"/>
																<p:commandButton id="mproprBtnMrc" icon="ui-icon-search"
																	oncomplete="PF('dialogoMgenusMarcaComercial').show();"
																	update=":formDetalle:dTableMgenusMarcaComercial"
																	process="@this">
																	<f:setPropertyActionListener target="#{seMgenus00.objSeCall}" value="#{seMpropr00}" />
																	<f:setPropertyActionListener target="#{seMgenus00.nombreTabla}" value="#{seMgenus00.tabla}" />
																	<f:setPropertyActionListener target="#{seMgenus00.nombreLista}" value="marcaComercial" />
																	<f:setPropertyActionListener target="#{seMgenus00.update}" value=":formDetalle:tabView:idmrprColumn, :formDetalle:tabView:idmrprColumnDesc" />
																</p:commandButton>
															</p:column>
														</p:row>
					
														<!-- Linea productos FK IDLIPR -->
														<p:row>
															<p:column>
																<h:outputLabel for="idliprColumn" value="#{seMpropr00.idiomasHm.get('idlipr')}" />
															</p:column>
															<p:column>
															<p:autoComplete id="idliprColumn" value="#{seMpropr00.mpropr00.mgenus002}" 
																	var="p" itemLabel="#{p.codius}" itemValue="#{p}"
																	converter="#{mgenus00Converter}" completeMethod="#{seMgenus00.completeMgenus}"
																	maxlenght="20" size="20"  forceSelection="true" scrollHeight="250"
																	placeholder="#{mensajes.placeholder_buscar}"
																	label="#{seMpropr00.idiomasHm.get('idlipr')}" >								
