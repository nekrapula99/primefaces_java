													<f:attribute name="tqigqg00" value="#{tqigqg00}" />
												</p:commandLink>
											</p:column>
					
											<!-- Numero Cotización -->
											<p:column headerText="#{seTqigqg00.idiomasHm.get('nroqqg')}"
