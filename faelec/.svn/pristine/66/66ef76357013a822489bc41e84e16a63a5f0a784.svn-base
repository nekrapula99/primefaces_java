										<p:remoteCommand autoRun="true">
											<f:setPropertyActionListener target="#{seCabecera.objCall}" value="#{seTqigqg00}" />
											<f:setPropertyActionListener target="#{seCabecera.varAdicional}" value="#{tqigqg00}" />
											<f:setPropertyActionListener target="#{seCabecera.pac}" value="pacTqigqg00" />
