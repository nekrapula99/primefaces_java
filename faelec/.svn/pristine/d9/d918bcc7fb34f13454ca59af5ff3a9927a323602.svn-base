package com.tlm.faelec.enums;


public enum MensajesEnum {
	Login_Cam_Obliga		("Login_Cam_Obliga", null),
	Login_UsuClave			("Login_UsuClave", null),
	Login_ActClave			("Login_ActClave", null),
	Login_UsuEmail			("Login_UsuEmail", null),
	Login_ProcesoFallo		("Login_ProcesoFallo", null),
	Login_EnviarCorrreo		("Login_EnviarCorrreo", null);

	MensajesEnum (String codigo, String mensaje) { 
		this.codigo = codigo;
		this.mensaje = mensaje;
    } 

	private String codigo;
	private String mensaje;
	
	public String getCodigo() {
		return codigo;
	}

	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}

	public String getMensaje() {
		return mensaje;
	}

	public void setMensaje(String mensaje) {
		this.mensaje = mensaje;
	}
	
}
