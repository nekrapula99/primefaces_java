public void removeBorrador() {
	try {
		tfbcbc00Aux = new Tfbcbc00();
		tfbcbc00Aux = consultarObjetoTfbcbc00(tfbcbc00.getTqdfqf00());
		if(tfbcbc00Aux!=null){
			
