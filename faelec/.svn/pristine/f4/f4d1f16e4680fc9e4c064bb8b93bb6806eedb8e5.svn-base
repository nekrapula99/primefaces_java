								</p:column>
								
								<!-- VALOR COTIZACIÓN -->
								
								<p:column>
									<h:outputLabel for="vacoqgColumnFp" value="#{seTqigqg00.idiomasHm.get('vacoqg')}:  " />
								</p:column>
