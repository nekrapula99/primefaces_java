											<h:outputText id="descqgColumnFp" value="#{seTqigqg00.tqigqg00.descqg}" />
										</p:column>
									
									
									<!--  Funcionario Comercial Responsable FK a MFUNFU001 -->
									
									
										<p:column>
											<h:outputLabel for="idfrqgColumnFp" value="#{seTqigqg00.idiomasHm.get('idfrqg')}:  " />
