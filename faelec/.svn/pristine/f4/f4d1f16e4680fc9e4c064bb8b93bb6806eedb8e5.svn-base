								<f:setPropertyActionListener target="#{seMgenus00.update}" 	value=":formTqdfqf00Detalle:idmqqfColumn, :formTqdfqf00Detalle:idmqqfColumnDesc" />
							</p:commandButton>
						</p:column>
						
						
					</p:row>
					
					
			
					<p:row>
