							</p:inputText>
							
							<p:inputText id="nrcpqfColumnRen" readonly="true" value="#{seTqdfqf00.tqdfqf00.nrcpqf}" rendered="#{seTqdfqf00.numModificar}"
										label="#{seTqdfqf00.idiomasHm.get('nrcpqf')}"
