
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:p="http://primefaces.org/ui"
                template="/WEB-INF/template.xhtml"
                xmlns:pe="http://primefaces.org/ui/extensions">

    <ui:define name="content">
    
        <div class="ui-g">
        
            <div class="ui-g-12">
                  
                <div class="card">
                
						<h1>#{seMparca00.titulo}</h1>
				
								<!-- //////////////////////////////////////////////////////////////////////////////// -->
								<!-- //////////////////////////////CABECERA////////////////////////////////////////// -->
								<h:form id="formCabecera">
									<p:remoteCommand autoRufn="true">
										<f:setPropertyActionListener target="#{seCabecera.objCall}" value="#{seMparca00}" />
										<f:setPropertyActionListener target="#{seCabecera.varAdicional}" value="#{mparca00}" />
										<f:setPropertyActionListener target="#{seCabecera.pac}" value="pacMparca00" />
										<f:setPropertyActionListener target="#{seCabecera.seControl.titulo}" value="#{seMparca00.titulo}" />
										<f:event type="preRenderView" listener="#{seMparca00.preRenderView}"  />
									</p:remoteCommand>
									<ui:include src="/pages/plantillas/cabecera.xhtml" />
								</h:form>		
						
						<h:form id="formNuevoUpdate">
									<p:remoteCommand name="rcNuevoUpdate"
										oncomplete="PF('dlgNuevoUpdate').show();"
										actionListener="#{seMparca00.accionNuevo}" update=":formDetalle" />
						</h:form>
						
				
						<!-- //////////////////////////////////////////////////////////////////////// -->
						<!-- //////////////////////////////// DATATABLE ///////////////////////////// -->
						<h:form id="formTable">
							
							<p:messages id="messages" showDetail="false" autoUpdate="false" closable="true" />
				
							<p:dataTable id="dTable" widgetVar="fTable" var="mparca00" reflow="true"
								paginator="true" paginatorPosition="bottom" rows="10"
								rowKey="#{mparca00.idtrca}" scrollable="true" emptyMessage="#{mensajes.grid_emptyMessage}"
								value="#{seMparca00.listMparca00}" selectionMode="single"
								selection="#{seMparca00.mparca00}"
								filteredValue="#{seMparca00.filteredMparca00}"
								resizableColumns="true"
								currentPageReportTemplate="#{mensajes.grid_total_registros} {totalRecords}"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink}">
				
								<p:ajax disabled="#{seCabecera.seControl.PERMISOSACCION.get('BTN_ACTUALIZAR')==null}" event="rowSelect" listener="#{seMparca00.accionModificar}"
									oncomplete="PF('dlgNuevoUpdate').show();" update=":formDetalle" />
				
								
								<p:column style="width:14px;" exportable="false">
										<f:facet name="header">
											<p:commandLink id="clarFilterLink"	onclick="PF('fTable').clearFilters();" actionListener="#{seMparca00.clearAllFilters}">
											<p:graphicImage value="/resources/images/clean_filters.png"  height="16" title="#{mensajes.boton_limpiar_filtros}" />
											</p:commandLink>
										</f:facet>
									<p:commandLink disabled="#{seCabecera.seControl.PERMISOSACCION.get('BTN_ELIMINAR')==null}"  id="deleteLink" update=":formTable" actionListener="#{seMparca00.remove}" process="@this">
										<p:graphicImage value="/resources/images/Trash.gif" style="border:0;" height="16" title="#{mensajes.boton_eliminar}" />
										<p:confirm header="#{mensajes.confirm_titulo}" message="#{mensajes.confirm_mensaje_remove}" icon="ui-icon-alert" />
										<f:attribute name="mparca00" value="#{mparca00}" />
									</p:commandLink>
								</p:column>
				                
						 					<!-- Documento -->
								<p:column headerText="#{seMparca00.idiomasHm.get('iddcca')}" sortBy="#{mparca00.mgenus001.codius}"
										 filterBy="#{mparca00.mgenus001.codius} #{mparca00.mgenus001.dcttus}" filterMatchMode="contains">
									<h:outputText value="#{mparca00.mgenus001.codius} #{mparca00.mgenus001.dcttus}" />
								</p:column>
								
											<!-- Idioma -->
								<p:column headerText="#{seMparca00.idiomasHm.get('idioca')}" sortBy="#{mparca00.midiom00.codidi}"
										filterBy="#{mparca00.midiom00.codidi} #{mparca00.midiom00.nomidi}" filterMatchMode="contains">
									<h:outputText value="#{mparca00.midiom00.codidi} #{mparca00.midiom00.nomidi}"/>					
								</p:column>
								
										<!-- Código de la compañia -->
								<p:column headerText="#{seMparca00.idiomasHm.get('idcmca')}" sortBy="#{mparca00.mconca00.codcia}"
										filterBy="#{mparca00.mconca00.codcia} #{mparca00.mconca00.nomcia}" filterMatchMode="contains">
									<h:outputText value="#{mparca00.mconca00.codcia} #{mparca00.mconca00.nomcia}" />
						    	</p:column>
				
									<!-- Registro activo -->
								<p:column headerText="#{seMparca00.idiomasHm.get('regica')}" filterBy="#{mparca00.regica}"
									filterMatchMode="contains" sortBy="#{mparca00.regica}" width="150" exportable="false">
									<f:facet name="filter">
										<p:selectOneButton onchange="PF('fTable').filter()">
											<f:converter converterId="javax.faces.Boolean" />
											<f:selectItem itemLabel="Todo" itemValue="" />
											<f:selectItem itemLabel="S" itemValue="true" />
											<f:selectItem itemLabel="N" itemValue="false" />
										</p:selectOneButton>
									</f:facet>
									<h:outputText
										value="#{mparca00.regica ? seTemplate.seControl.rb.getString('mj_activo_si') : seTemplate.seControl.rb.getString('mj_activo_no')}" />
								</p:column>
							</p:dataTable>
						</h:form>
					
				        <h:form id="formMparpd00Detalle">
				        
				 				<p:dialog header="#{seMparpd00.titulo}" resizable="false" responsive="true" widgetVar="dlgNuevoUpdateMparpd00" 
				 						  height="#{seTemplate.MAX_HEIGHT_DIALOG}" width="#{seTemplate.MAX_WIDTH_DIALOG}" modal="true" >
									
									<p:ajax event="close" listener="#{seTemplate.seControl.evtCloseDialog}" />
									
									<h:panelGrid id="pnlDetalleMparpd" columns="1">
									
										<p:messages id="messages" showDetail="false" autoUpdate="true"
											closable="true" />
										<p:panelGrid id="pnlMparpd00Detalle" styleClass="ui-panelgrid-blank">
									
													<!-- Codigo de campo -->
											<p:row>
												<p:column>
													<h:outputLabel for="codcpdColumn" value="#{seMparpd00.idiomasHm.get('codcpd')}" />
												</p:column>
												<p:column>
													<p:inputTextarea id="codcpdColumn" value="#{seMparpd00.mparpd00.codcpd}" maxlength="20"
														label="#{seMparpd00.idiomasHm.get('codcpd')}" rows="1" cols="20" required="true"
														requiredMessage="#{seMparpd00.idiomasHm.get('codcpd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">									
													</p:inputTextarea>
												</p:column>
											</p:row>	
																		
													<!-- Descripcion del campo -->
											<p:row>
												<p:column>
													<h:outputLabel for="dscapdColumn" value="#{seMparpd00.idiomasHm.get('dscapd')}" />
												</p:column>
												<p:column>
													<p:inputTextarea id="dscapdColumn" value="#{seMparpd00.mparpd00.dscapd}" maxlength="50"
														label="#{seMparpd00.idiomasHm.get('dscapd')}" style="width:100%" required="true"  rows="2"
													    requiredMessage="#{seMparpd00.idiomasHm.get('dscapd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}" />
												</p:column>
											</p:row>
											<!-- Imprimir campo en el reporte -->
											<p:row>
												<p:column>
													<h:outputLabel for="inrppdColumn" value="#{seMparpd00.idiomasHm.get('inrppd')}" />
												</p:column>
												<p:column>
													<p:selectBooleanCheckbox id="inrppdColumn" label="#{seMparpd00.idiomasHm.get('inrppd')}" 
																		value="#{seMparpd00.mparpd00.inrppd}" />
												</p:column>
											</p:row>							
											  <!-- Descripcion Reporte -->
											<p:row>
												<p:column>
													<h:outputLabel for="dsrapdColumn" value="#{seMparpd00.idiomasHm.get('dsrapd')}" />
												</p:column>
												<p:column>
													<p:inputTextarea id="dsrapdColumn" 	value="#{seMparpd00.mparpd00.dsrapd}" maxlength="100"
														label="#{seMparpd00.idiomasHm.get('dsrapd')}" style="width:100%" rows="3" cols="35"
														required="#{seMparpd00.permisoCampos.get('dsrapd').reqcam.equals('S')}"
														requiredMessage="#{seMparpd00.idiomasHm.get('dsrapd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}" />
												</p:column>
											</p:row>
											<!-- Descripcion reporte en negrita -->
											<p:row>
												<p:column>
													<h:outputLabel for="indnpdColumn" value="#{seMparpd00.idiomasHm.get('indnpd')}" />								
												</p:column>
												<p:column>
													<p:selectBooleanCheckbox id="indnpdColumn" label="#{seMparpd00.idiomasHm.get('indnpd')}"
														value="#{seMparpd00.mparpd00.indnpd}" />
												</p:column>
											</p:row>
											<!-- Contenido campo -->	
											<p:row>
												<p:column>
													<h:outputLabel for="inccpdColumn" value="#{seMparpd00.idiomasHm.get('inccpd')}" />
												</p:column>
												<p:column>
													<p:selectOneMenu id="inccpdColumn" value="#{seMparpd00.mparpd00.inccpd}"
														label="#{seMparpd00.idiomasHm.get('inccpd')}" style="width:125px"
														required="#{seMparpd00.permisoCampos.get('inccpd').reqcam.equals('S')}"
														requiredMessage="#{seMparpd00.idiomasHm.get('inccpd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
														<f:selectItem itemLabel="Valor real" itemValue="1" />
														<f:selectItem itemLabel="Valor constante" itemValue="2" />
														<f:selectItem itemLabel="Blanco" itemValue="3" />
														<f:selectItem itemLabel="Cero" itemValue="4" />
														<p:ajax listener="#{seMparpd00.requiredValorConstante}" />
													</p:selectOneMenu>
												</p:column>
											</p:row>	
											<!-- Valor constante -->
											<p:row>
												<p:column>
													<h:outputLabel for="vrccpdColumn" value="#{seMparpd00.idiomasHm.get('vrccpd')}" />
												</p:column>
												<p:column>
													<p:inputText id="vrccpdColumn" value="#{seMparpd00.mparpd00.vrccpd}"
														label="#{seMparpd00.idiomasHm.get('vrccpd')}" style="width:100%" 
														required="#{seMparpd00.permisoCampos.get('vrccpd').reqcam.equals('S') || seMparpd00.requiredVrccpd}"
														requiredMessage="#{seMparpd00.idiomasHm.get('vrccpd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}"/>
												</p:column>
											</p:row>													
																	
											<!-- Numero Decimales -->
											<p:row>
												<p:column>
													<h:outputLabel for="nrdepdColumn" value="#{seMparpd00.idiomasHm.get('nrdepd')}" />
												</p:column>
												<p:column>
													<p:inputTextarea id="nrdepdColumn" value="#{seMparpd00.mparpd00.nrdepd}" maxlength="5"
														label="#{seMparpd00.idiomasHm.get('nrdepd')}" rows="1" cols="6"
														required="#{seMparpd00.permisoCampos.get('nrdepd').reqcam.equals('S')}"
														requiredMessage="#{seMparpd00.idiomasHm.get('nrdepd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
														<pe:keyFilter regEx="/[\d\-]/" />
													</p:inputTextarea>
												</p:column>
											</p:row>
											<!-- Campo negrita -->
											<p:row>
												<p:column>
													<h:outputLabel for="incnpdColumn" value="#{seMparpd00.idiomasHm.get('incnpd')}" />
												</p:column>
												<p:column>
													<p:selectBooleanCheckbox id="incnpdColumn" label="#{seMparpd00.idiomasHm.get('incnpd')}"
																			value="#{seMparpd00.mparpd00.incnpd}"  />
												</p:column>
											</p:row>
											
											<!-- Fuente -->
											<p:row>
												<p:column>
													<h:outputLabel for="fuenpdColumn" value="#{seMparpd00.idiomasHm.get('fuenpd')}" />
												</p:column>
												<p:column>
											        <p:selectOneMenu id="fuenpdColumn" value="#{seMparpd00.fuentes}" var="fuente" converter="fuentesConverter">
											            <f:selectItems value="#{seMparpd00.listaFuentes}" var="t" itemLabel="#{t.nombreFuente}" itemValue="#{t}" />
												         <p:column>
														  	<h:outputText value="#{fuente.nombreFuente}" style="font-family:#{fuente.nombreFuente}"/>
														 </p:column>
											        </p:selectOneMenu>
										        </p:column>
											</p:row>
											
											<!-- Tamaño fuente -->
											<p:row>
												<p:column>
													<h:outputLabel for="tamapdColumn" value="#{seMparpd00.idiomasHm.get('tamapd')}" />
												</p:column>
												<p:column>
													<p:inputTextarea id="tamapdColumn" value="#{seMparpd00.mparpd00.tamapd}" maxlength="2"
														label="#{seMparpd00.idiomasHm.get('tamapd')}" rows="1" cols="6"
														required="#{seMparpd00.permisoCampos.get('tamapd').reqcam.equals('S')}"
														requiredMessage="#{seMparpd00.idiomasHm.get('tamapd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
														<pe:keyFilter regEx="/[\d\-]/" />
													</p:inputTextarea>
												</p:column>
											</p:row>
											<!-- Color Fuente -->
											<p:row>
												<p:column>
													<h:outputLabel for="colopdColumn" value="#{seMparpd00.idiomasHm.get('colopd')}" />
												</p:column>
												<p:column>
													<p:colorPicker id="colopdColumn" value="#{seMparpd00.colorFuente}" maxlength="20"
														label="#{seMparpd00.idiomasHm.get('colopd')}" style="width:100%"
														required="#{seMparpd00.permisoCampos.get('colopd').reqcam.equals('S')}"
														requiredMessage="#{seMparpd00.idiomasHm.get('colopd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}" />
												</p:column>
											</p:row>
											<!-- Registro activo -->
											<p:row>		
												<p:column>
													<h:outputLabel for="regipdColumn" value="#{seMparpd00.idiomasHm.get('regipd')}" />
												</p:column>
												<p:column>
													<p:selectBooleanCheckbox id="regipdColumn" label="#{seMparpd00.idiomasHm.get('regipd')}" 
															value="#{seMparpd00.mparpd00.regipd}" />
												</p:column>
										   </p:row>																
				
										</p:panelGrid>
										<!-- Logo de la empresa seMconca00.mconca00.logcia-->
										<p:separator />
				
										<h:panelGrid id="pnlDetalle2" columns="2">
											<h:outputLabel value="#{seMparpd00.idiomasHm.get('logopd')}" />
											<p:spacer />
				
											<p:fileUpload style="width: 300px"
												fileUploadListener="#{seMparpd00.uploadLogoFile}"
												multiple="false" mode="advanced" auto="true"
												label="#{mensajes.boton_adjuntar}" update="pnlDetalle2"
												required="#{seMparpd00.permisoCampos.get('logopd').reqcam.equals('S')}"
												requiredMessage="#{seMparpd00.idiomasHm.get('logopd')} : #{seMparpd00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
											</p:fileUpload>
											<p:graphicImage value="#{seMparpd00.logoStreamedContent}"
												width="120px" height="80px"
												rendered="#{seMparpd00.mparpd00.logopd != null}" cache="false" />
											<p:graphicImage value="/resources/images/no-image.png"
												width="120px" height="80px"
												rendered="#{seMparpd00.mparpd00.logopd == null}" cache="false" />
										</h:panelGrid>
									</h:panelGrid>
									<f:facet name="footer">
										<p:commandButton value="#{mensajes.boton_guardar}"                      
											actionListener="#{seMparpd00.save}"	
											update=":formMparpd00Detalle:pnlDetalleMparpd,:formDetalle:tabView:dTableMparpd00" >
											 <p:confirm header="#{mensajes.confirm_titulo}" message="#{mensajes.confirm_mensaje_save}" icon="ui-icon-alert" />
										</p:commandButton>
									</f:facet>
								</p:dialog>       
				   	
				        </h:form>
					<!-- //////////////////////////////////////////////////////////////////////// -->
					<!-- /////////////////////// ACTUALIZAR - MODIFICAR ///////////////////////// -->
					
					<!-- Dialogo Mparca00 -->
					<h:form id="formDetalle">
					
						<ui:include src="/pages/listas/palmconca.xhtml" />
						<ui:include src="/pages/listas/palmgenusDocumentos.xhtml" />
						<ui:include src="/pages/listas/palmidiom.xhtml" />
						<ui:include src="/pages/listas/palmgenterlegal.xhtml" />
						<ui:include src="/pages/listas/palmtiptx.xhtml" />
						
							<p:dialog header="#{seMparca00.titulo}" widgetVar="dlgNuevoUpdate" resizable="false" modal="true"
							   		  height="#{seTemplate.MAX_HEIGHT_DIALOG}" width="#{seTemplate.MAX_WIDTH_DIALOG}" responsive="true">
							   		  
									<p:ajax event="close" listener="#{seTemplate.seControl.evtCloseDialog}" />
									
									<p:messages id="messages" showDetail="false" autoUpdate="true" closable="true" />
									
										<p:tabView id="tabView" activeIndex="#{seMparca00.tabView}">
											<p:tab title="#{seMparca00.idiomasHm.get('infgrl')}" id="tabInformacionGeneral">
												<h:panelGrid id="pnlDetalle" columns="1">
												
												<p:panelGrid id="pnlMparca00" styleClass="ui-panelgrid-blank">
												
													<!--  Codigo compañia FK a MCONCA00 -->
													<p:row>
														<p:column>
															<p:outputLabel for="idcmcaColumn" value="#{seMparca00.idiomasHm.get('idcmca')}"/>
														</p:column>
														<p:column>
															<p:autoComplete id="idcmcaColumn"
																value="#{seMparca00.mparca00.mconca00}" var="p"
																itemLabel="#{p.codcia}" itemValue="#{p}"
																converter="#{mconca00Converter}"
																completeMethod="#{seMconca00.completeMconca}" maxlenght="20"
																size="20" forceSelection="true" scrollHeight="250"
																placeholder="#{mensajes.placeholder_buscar}"
																label="#{seMparca00.idiomasHm.get('idcmca')}" 
																required="#{seMparca00.permisoCampos.get('idcmca').reqcam.equals('S')}"
																requiredMessage="#{seMparca00.idiomasHm.get('idcmca')} : #{seMmesje00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">
																<p:ajax event="itemSelect" update="idcmcaColumnDesc, compania" />
																<p:ajax event="blur" update="idcmcaColumnDesc, compania" />
																<p:ajax listener="#{seMparca00.autocompleteActualizarListas}" />
															</p:autoComplete>
															<p:inputText id="idcmcaColumnDesc" value="#{seMparca00.mparca00.mconca00.nomcia}" readonly="true" />	
															<p:commandButton id="mparcaBtnCompa" icon="ui-icon-search" oncomplete="PF('dialogoMconca00').show();"
																			 update=":formDetalle:dTableMconca00List" process="@this">
																<f:setPropertyActionListener target="#{seMconca00.objSeCall}" value="#{seMparca00}" />
																<f:setPropertyActionListener target="#{seMconca00.nombreTabla}" value="#{seMconca00.tabla}" />
																<f:setPropertyActionListener target="#{seMconca00.nombreLista}" value="companias" />
																<f:setPropertyActionListener target="#{seMconca00.update}" value=":formDetalle:tabView:idcmcaColumn, :formDetalle:tabView:idcmcaColumnDesc,
																										:formDetalle:tabView:compania" />
															</p:commandButton>
														</p:column>
													</p:row>	
																					
													<p:row>
														<!-- Documento FK MGENUS00-->
														<p:column>
															<h:outputLabel for="iddccaColumn" value="#{seMparca00.idiomasHm.get('iddcca')}" />
														</p:column>
														<p:column>
															<p:autoComplete id="iddccaColumn" value="#{seMparca00.mparca00.mgenus001}" 
																		var="p" itemLabel="#{p.codius}" itemValue="#{p}"
																		converter="#{mgenus00Converter}" completeMethod="#{seMgenus00.completeMgenus}"
																		maxlenght="20" size="20"  forceSelection="true" scrollHeight="250"
																		placeholder="#{mensajes.placeholder_buscar}"
																		label="#{seMparca00.idiomasHm.get('iddcca')}" required="true"
																		requiredMessage="#{seMparca00.idiomasHm.get('iddcca')} : #{seMmesje00.getSeControlFactura().MENSAJES.get('Cam_Obliga')}">								
																		<p:ajax event="itemSelect" update="iddccaColumnDesc, documento" />
																		<f:attribute name="codtus" value="#{params.mgenus_codtus_documento}" />
																		<p:ajax event="blur" update="iddccaColumnDesc, documento" />
															</p:autoComplete>
															<p:inputText id="iddccaColumnDesc" value="#{seMparca00.mparca00.mgenus001.dcttus}" readonly="true"/>
															<p:commandButton id="mgenusBtn" icon="ui-icon-search" 	oncomplete="PF('dialogoMgenusDocumentos').show();"
